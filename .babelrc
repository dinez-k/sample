{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
        [
          "babel-plugin-root-import",
          {
            "paths": [
              {
                "rootPathSuffix": "src",
                "rootPathPrefix": "~"
              },
              {
                "rootPathSuffix": "node_modules/@mui/material",
                "rootPathPrefix": "@mui/material"
              }
            ]
          }
        ]
      ]
}